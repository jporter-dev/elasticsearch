{
  "name": "@jporter-dev/nestjs-opensearch",
  "version": "1.0.0",
  "description": "Nest - modern, fast, powerful node.js web framework (@opensearch-project/opensearch)",
  "author": "Josh Porter",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint \"lib/**/*.ts\" --fix",
    "format": "prettier \"lib/**/*.ts\" --write",
    "build": "rimraf -rf dist && tsc -p tsconfig.json",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "publish:npm": "npm publish --access public",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.0.0 || ^7.0.0 || ^8.0.0 || ^9.0.0",
    "@opensearch-project/opensearch": "^1.1.0 || ^2.0.0",
    "rxjs": "^6.2.1 || ^7.2.0"
  },
  "devDependencies": {
    "@nestjs/common": "9.0.11",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-angular": "17.0.3",
    "@opensearch-project/opensearch": "2.0.0",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "9.0.1",
    "eslint": "8.23.0",
    "@types/node": "16.11.56",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "husky": "8.0.1",
    "release-it": "15.4.0",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "rxjs": "7.5.6",
    "typescript": "4.7.4"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "dependencies": {},
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
